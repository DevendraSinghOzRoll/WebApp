    function SpecialLockOptionsEnableDisable() {
        var flushBoltTopDDL = document.getElementById("<%=cboFlushBoltsTop.ClientID %>");
        var flushBoltBottomDDL = document.getElementById("<%=cboFlushBoltsBottom.ClientID %>");
        var lockOptionsDDL = document.getElementById("<%=cboLockOptions.ClientID %>");
        var shutterTypeVal = document.getElementById("<%=cboShutterType.ClientID %>").value;

        // Can only be enabled if BiFold, Stacker, or Hinged.
        if ((shutterTypeVal == 1) || (shutterTypeVal == 6) || (shutterTypeVal == 11) || (shutterTypeVal == 12) || (shutterTypeVal == 13)) {

            if ((flushBoltTopDDL.value <= 0) && (flushBoltBottomDDL.value <= 0) && (lockOptionsDDL.value <= 0)) {
                // All boxes are 0 so switch lock options on.
                lockOptionsDDL.disabled = false;
                lockOptionsDDL.className = "form-select";
            }
            else {
                if ((flushBoltTopDDL.value > 0) || (flushBoltBottomDDL.value > 0)) {
                    // Flush bolt is enabled and has a selected value so disable lock options
                    lockOptionsDDL.value = 0;
                    lockOptionsDDL.disabled = true;
                    lockOptionsDDL.className = "form-select-disable";
                }
                else {
                    // Flush bolt is enabled and has NO selected value so enable lock options
                    lockOptionsDDL.disabled = false;
                    lockOptionsDDL.className = "form-select";
                }
            }
        }
        else {
            // Disable for these shutter types.
            lockOptionsDDL.value = 0;
            lockOptionsDDL.disabled = true;
            lockOptionsDDL.className = "form-select-disable";
        }
    }

    function FlushBoltsEnableDisable() {
        var flushBoltTopDDL = document.getElementById("<%=cboFlushBoltsTop.ClientID %>");
        var flushBoltBottomDDL = document.getElementById("<%=cboFlushBoltsBottom.ClientID %>");
        var lockOptionsDDL = document.getElementById("<%=cboLockOptions.ClientID %>");
        var shutterTypeVal = document.getElementById("<%=cboShutterType.ClientID %>").value;
        
        // Can only be enabled if BiFold, Stacker, or Hinged.
        if ((shutterTypeVal == 1) || (shutterTypeVal == 6) || (shutterTypeVal == 11) || (shutterTypeVal == 12) || (shutterTypeVal == 13)) {

            if ((flushBoltTopDDL.value <= 0) && (flushBoltBottomDDL.value <= 0) && (lockOptionsDDL.value <= 0)) {
                // All boxes are 0 so switch flush bolts on.
                flushBoltTopDDL.disabled = false;
                flushBoltTopDDL.className = "form-select";
                flushBoltBottomDDL.disabled = false;
                flushBoltBottomDDL.className = "form-select";
            }
            else {
                if (lockOptionsDDL.value > 0) {
                    // Lock options is enabled and has a selected value so disable bolts.
                    flushBoltTopDDL.value = 0;
                    flushBoltTopDDL.disabled = true;
                    flushBoltTopDDL.className = "form-select-disable";
                    flushBoltBottomDDL.value = 0;
                    flushBoltBottomDDL.disabled = true;
                    flushBoltBottomDDL.className = "form-select-disable";
                }
                else {
                    // Lock options is enabled and has NO selected value so enable bolts.
                    flushBoltTopDDL.disabled = false;
                    flushBoltTopDDL.className = "form-select";
                    flushBoltBottomDDL.disabled = false;
                    flushBoltBottomDDL.className = "form-select";
                }
            }
        }
        else {
            // Disable for these shutter types.
            flushBoltTopDDL.value = 0;
            flushBoltTopDDL.disabled = true;
            flushBoltTopDDL.className = "form-select-disable";
            flushBoltBottomDDL.value = 0;
            flushBoltBottomDDL.disabled = true;
            flushBoltBottomDDL.className = "form-select-disable";
        }
    }

