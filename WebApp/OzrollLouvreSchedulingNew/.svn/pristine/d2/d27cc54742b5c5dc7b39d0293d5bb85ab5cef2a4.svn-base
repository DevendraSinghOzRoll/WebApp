            function checkDates(datetextbox1, datetextbox2) {
                var date1 = new date(datetime.parse(datetextbox1.value));
                var date2 = new date(datetime.parse(datetextbox2.value));

                var intDays = dayDiff(date1, date2);

                if (intDays < 0) {
                    datetextbox2.value == '';
                    return false;
                }
                else {
                    return true;
                }              
            }

            function dayDiff(startdate, enddate) {
                var dayCount = 0;

                while (enddate >= startdate) {
                    dayCount++;
                    startdate.setDate(startdate.getDate() + 1);
                }

                return dayCount;
            }

            function CalculateNumberOfPanelsForOpening() {
            var ShutterType = 0;
            if (!(document.getElementById("<%=cboShutterType.ClientID %>").value == "0")) {
                ShutterType = document.getElementById("<%=cboShutterType.ClientID %>").value;
            }

            var nopanelid = 0;
            if (!(document.getElementById("<%=cboNoOfPanels.ClientID %>").value == "0")) {
                nopanelid = document.getElementById("<%=cboNoOfPanels.ClientID %>").value;
            }

            var width = document.getElementById("<%=txtWidth.ClientID %>").value;

            var numPanels = 0;
            
            if (nopanelid < 1) {
                
                if (ShutterType == 1) { //bifold
                    numPanels = Math.ceil(width / 900);
                }

                if (ShutterType == 2 || ShutterType == 3 || ShutterType == 4 || ShutterType == 5) { //slider
                    numPanels = Math.ceil(width / 1100);
                }

                if (ShutterType == 6) { //stacker
                    numPanels = Math.ceil(width / 1100);
                }

                if (ShutterType == 7 || ShutterType == 8 || ShutterType == 9 || ShutterType == 10 || ShutterType == 14) { //fixed (14=standoff)
                    numPanels = Math.ceil(width / 1100);
                }

                if (ShutterType == 11 || ShutterType == 12 || ShutterType == 13) { //hinged
                    numPanels = Math.ceil(width / 500);
                }
            }
            return numPanels;
        }

        function PanelWidthIsValid(source, args) {
            
            var ShutterType = 0;
            if (!(document.getElementById("<%=cboShutterType.ClientID %>").value == "0")) {
                ShutterType = document.getElementById("<%=cboShutterType.ClientID %>").value;
            }

            var nopanelid = 0;
            if (!(document.getElementById("<%=cboNoOfPanels.ClientID %>").value == "0")) {
                nopanelid = document.getElementById("<%=cboNoOfPanels.ClientID %>").value;
            }

            var width = document.getElementById("<%=txtWidth.ClientID %>").value;
            
            if ((nopanelid == "") || (nopanelid == "0")) {
                
                // Cant have more than 2 panels when Hinges 2 panels selected.
                switch (ShutterType) {
                    case "13": // LouvreTypes.Hinged2Panels
                        if (CalculateNumberOfPanelsForOpening() > 2) {
                            args.IsValid = false;
                            return;
                        }

                    case "11", "12": // LouvreTypes.HingedPanelLeft, LouvreTypes.HingedPanelRight
                        if (CalculateNumberOfPanelsForOpening > 1) {
                            args.IsValid = false;
                            return;
                        }
                }
            }
            args.IsValid = true;
        }

