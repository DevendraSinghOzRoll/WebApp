    function flyScreenActive() {


        var flyScreen = 0;
        {
            flyScreen = document.getElementById("<%=cboFlyScreen.ClientID %>").value = 0;
        }

        var InOut = document.getElementById("<%=cboBiFoldHingedDoor.ClientID %>");

        if (InOut.options[InOut.selectedIndex].text == "In") {
            document.getElementById("<%=cboFlyScreen.ClientID %>").options.length = 0;

            document.getElementById("<%=cboFlyScreen.ClientID %>").disabled = true;
            document.getElementById("<%=cboFlyScreen.ClientID %>").className = "form-select-disable";

        }
        else if (InOut.options[InOut.selectedIndex].text == "Out") {
            document.getElementById("<%=cboFlyScreen.ClientID %>").options.length = 0;
            document.getElementById("<%=cboFlyScreen.ClientID %>").disabled = false;
            document.getElementById("<%=cboFlyScreen.ClientID %>").className = "form-select";

            var myArray = new Array("Yes", "No");


            // Get dropdown element from DOM
            var dropdown = document.getElementById("<%=cboFlyScreen.ClientID %>");

            // Loop through the array
            for (var i = 0; i < myArray.length; ++i) {
                // Append the element to the end of Array list
                dropdown[dropdown.length] = new Option(myArray[i], myArray[i]);
            }  

            if (flyScreen != 0) {
                document.getElementById("<%=cboFlyScreen.ClientID %>").value = flyScreen;
            }
        }
        else {

            document.getElementById("<%=cboFlyScreen.ClientID %>").options.length = 0;

            document.getElementById("<%=cboFlyScreen.ClientID %>").disabled = true;
            document.getElementById("<%=cboFlyScreen.ClientID %>").className = "form-select-disable";

        }
    }
     


    
