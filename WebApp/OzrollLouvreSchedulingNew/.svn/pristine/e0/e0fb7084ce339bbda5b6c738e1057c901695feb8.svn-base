        function stackEnable() {
            if (document.getElementById("<%=cboShutterType.ClientID %>").value == "0") {
                return false;
            }


            var ShutterType = 0;
            if (!(document.getElementById("<%=cboShutterType.ClientID %>").value == "0")) {
                ShutterType = document.getElementById("<%=cboShutterType.ClientID %>").value;
            }

            var Stack = 0;
            if (!(document.getElementById("<%=cboStacker.ClientID %>").value == "0")) {
                Stack = document.getElementById("<%=cboStacker.ClientID %>").value;
            }


            if (ShutterType == 3) {
                document.getElementById("<%=cboStacker.ClientID %>").disabled = false;
                document.getElementById("<%=cboStacker.ClientID %>").className = "form-select";
                document.getElementById("<%=txtStack.ClientID %>").value = 0;

                if (Stack != 0) {
                    document.getElementById("<%=cboStacker.ClientID %>").value = Stack;
                }

            }
