        function lockOptions() {
            var ShutterType = 0;
            if (!(document.getElementById("<%=cboShutterType.ClientID %>").value == "0")) {
                ShutterType = document.getElementById("<%=cboShutterType.ClientID %>").value;
            }

            var lock = 0;
            if (!(document.getElementById("<%=cboLockOptions.ClientID %>").value == "0")) {
                lock = document.getElementById("<%=cboLockOptions.ClientID %>").value;
            }

            var name = "Handle Lock";
            if (ShutterType == 5) {
                if (!($j("select[id$='cboLockOptions'] option:contains('" + name + "')").length > 0)) {
                    var lockOpt = document.forms[0].elements["<%=cboLockOptions.UniqueId%>"];
                    if (!lockOpt) {
                        return; //error
                    }
                    var option = document.createElement('OPTION');
                    option.value = 1;
                    option.innerHTML = name;
                    lockOpt.appendChild(option);
                }
            }
            else {
                if ($j("select[id$='cboLockOptions'] option:contains('" + name + "')").length > 0) {
                    $j("select[id$='cboLockOptions'] option[value='1']").remove();
                }
            }

            if (lock != 0) {
                document.getElementById("<%=cboLockOptions.ClientID %>").value = lock;
            }
        }



