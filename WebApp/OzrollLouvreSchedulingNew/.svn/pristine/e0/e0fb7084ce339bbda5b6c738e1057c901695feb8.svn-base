     function getValidPanel() {


         //            if (document.getElementById("<%=cboShutterType.ClientID %>").value == "0") {
            //                return false;
            //            }

            //            if (document.getElementById("<%=txtWidth.ClientID %>").value == "") {
            //                return false;
            //            }

            var ShutterType = 0;
            if (!(document.getElementById("<%=cboShutterType.ClientID %>").value == "0")) {
                ShutterType = document.getElementById("<%=cboShutterType.ClientID %>").value;
            }

            var nopanelid = 0;
            if (!(document.getElementById("<%=txtNoPanel.ClientID %>").value == "0")) {
                nopanelid = document.getElementById("<%=txtNoPanel.ClientID %>").value;
            }

            var width = document.getElementById("<%=txtWidth.ClientID %>").value;

            var nopanel = document.forms[0].elements["<%=cboNoOfPanels.UniqueId%>"];
            if (!nopanel) {
                return; //error
            }
            nopanel.options.length = 0;

            var widthm = 0;
            var lookupId = 0;

            if (ShutterType == 1) {
                widthm = width / 800;
                lookupId = 1;
            }

            if (ShutterType == 2 || ShutterType == 3) {
                widthm = width / 1000;
                lookupId = 0;
            }

            if (ShutterType == 4) {
                widthm = width / 1100;
                lookupId = 0;
            }

            if (ShutterType == 5) {
                widthm = width / 900;
                lookupId = 0;
            }

            for (var i = 0; i < PanelLookups.length; ++i) {
                var lookup = PanelLookups[i];
                if (lookup.opening >= widthm && lookup.typeId == lookupId) {
                    var option = document.createElement('OPTION');
                    option.value = lookup.value;
                    option.innerHTML = lookup.name;
                    nopanel.appendChild(option);
                }
            }

            if (nopanelid != 0) {
                nopanel.value = nopanelid;
            }

            var Slideid = 0;
            if (!(document.getElementById("<%=cboSlide.ClientID %>").value == "0")) {
                Slideid = document.getElementById("<%=cboSlide.ClientID %>").value;
            }

         

            if (ShutterType == 2) {
                document.getElementById("<%=cboSlide.ClientID %>").disabled = false;
                document.getElementById("<%=cboSlide.ClientID %>").className = "form-select";
                document.getElementById("<%=txtSlide.ClientID %>").value = 0;
                if (Slideid != 0) {
                    document.getElementById("<%=cboSlide.ClientID %>").value = Slideid;
                }
              
            }
            else {
                document.getElementById("<%=cboSlide.ClientID %>").disabled = true;
                document.getElementById("<%=cboSlide.ClientID %>").className = "form-select-disable";
                document.getElementById("<%=cboSlide.ClientID %>").value = "";
                document.getElementById("<%=txtSlide.ClientID %>").value = 1;    
            }
            //alert(document.getElementById("<%=cboNoOfPanels.ClientID %>").value);

        }

