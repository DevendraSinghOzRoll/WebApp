
Partial Class PackupDate
    Inherits System.Web.UI.Page

    Protected Sub Page_Load(sender As Object, e As EventArgs) Handles Me.Load

    End Sub

    Private Sub chkDoubleRail_CheckedChanged(sender As Object, e As EventArgs) Handles chkDoubleRail.CheckedChanged
        If chkDoubleRail.Checked Then
            lblCheckBoxStatus.Text = "Present"
        Else
            lblCheckBoxStatus.Text = "Not Present"
        End If
    End Sub
End Class
